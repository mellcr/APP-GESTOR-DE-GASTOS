<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.web>
		<compilation debug="true" targetFramework="4.8.1" />
		<httpRuntime targetFramework="4.8.1" />
	</system.web>
	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="IngredienteWSPortBinding" receiveTimeout="00:1:00" sendTimeout="00:1:00"
						maxBufferSize="2147483647" maxReceivedMessageSize="2147483647" />
				<binding name="PlatoWSPortBinding" receiveTimeout="00:1:00" sendTimeout="00:1:00"
						maxBufferSize="2147483647" maxReceivedMessageSize="2147483647"/>
			</basicHttpBinding>
		</bindings>
		<client>
			<!-- Configuración de los endpoints para los servicios -->
			<endpoint address="http://localhost:8080/SazonWareWSBL/IngredienteWS"
					  binding="basicHttpBinding" bindingConfiguration="IngredienteWSPortBinding"
					  contract="ServicioWS.IngredienteWS" name="IngredienteoWSPort" />
			<endpoint address="http://localhost:8080/SazonWareWSBL/PlatoWS"
					  binding="basicHttpBinding" bindingConfiguration="PlatoWSPortBinding"
					  contract="ServicioWS.PlatoWS" name="PlatoWSPort" />
		</client>
	</system.serviceModel>
</configuration>
